{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${version}",
  "name": "${name}",
  "description": "${description}",
  "authors": [
    "${author}"
  ],
  "contact": {
    "homepage": "https://github.com/Ayydxn/Multi-ModLoaderTemplate",
    "sources": "https://github.com/Ayydxn/Multi-ModLoaderTemplate",
    "issues": "${issue_tracker_url}"
  },
  "license": "${license}",
  "icon": "${icon_path}",
  "environment": "*",
  "entrypoints": {
    "main": [
      "me.ayydan.multimodloadertemplate.fabric.ExampleModFabric"
    ]
  },
  "mixins": [
    "${mod_id}-common.mixins.json",
    "${mod_id}.mixins.json"
  ],
  "depends": {
    "minecraft": ">=${minecraft_version}",
    "architectury": ">=${architectury_version}",
    "fabric": ">=${fabric_version}",
    "fabricloader": ">=${fabric_loader_version}"
  }
}